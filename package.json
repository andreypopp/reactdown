{
  "name": "reactdown",
  "version": "1.0.0",
  "description": "Markdown and React based live document format",
  "main": "lib/index.js",
  "scripts": {
    "test": "make lint check test"
  },
  "babel": {
    "presets": [
      "prometheusresearch"
    ],
    "plugins": [
      "ast-literal"
    ]
  },
  "eslintConfig": {
    "extends": "prometheusresearch",
    "env": {
      "mocha": true
    },
    "globals": {
      "$Shape": true,
      "expr": true,
      "stmt": true
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andreypopp/reactdown.git"
  },
  "keywords": [
    "react",
    "markdown"
  ],
  "author": "Andrey Popp <8mayday@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/andreypopp/reactdown/issues"
  },
  "homepage": "https://github.com/andreypopp/reactdown#readme",
  "dependencies": {
    "collapse-white-space": "^1.0.0",
    "commander": "^2.9.0",
    "detab": "^1.0.2",
    "flow-bin": "^0.23.1",
    "invariant": "^2.2.1",
    "js-yaml": "^3.5.5",
    "json5": "^0.5.0",
    "loader-utils": "^0.2.13",
    "normalize-uri": "^1.0.0",
    "remark": "^4.1.2",
    "resolve": "^1.1.7",
    "trim": "0.0.1",
    "trim-lines": "^1.0.0",
    "unist-util-visit": "^1.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-generator": "^6.7.2",
    "babel-plugin-ast-literal": "^0.4.0",
    "babel-preset-prometheusresearch": "^0.1.0",
    "babel-types": "^6.7.2",
    "eslint": "^2.5.3",
    "eslint-config-prometheusresearch": "^0.2.0",
    "flow-bin": "^0.23.0",
    "gh-pages": "^0.11.0",
    "mocha": "^2.4.5",
    "nyc": "^6.1.1",
    "sloc": "^0.1.10"
  }
}
