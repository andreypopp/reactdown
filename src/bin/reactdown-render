#!/usr/bin/env node

import fs from 'fs';
import {renderToString} from '../';

let filename = process.argv[2];

if (!filename) {
  console.error('usage: parse <filename>');
  process.exit(1);
}

let src = fs.readFileSync(filename, 'utf8');

console.log(renderToString(src).code);

// vim: set ft=javascript:
